    getactor[target_id].y MY
    getactor[target_id].extra enemyextra
    getactor[THISACTOR].x x
    getactor[THISACTOR].y y
    getactor[THISACTOR].ang angproj
    ifvarg enemyextra 0
    {
      subvarvar MX x
      subvarvar MY y

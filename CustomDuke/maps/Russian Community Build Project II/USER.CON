      ifvarl angproj 0 addvar angproj 2047
      ifvarg angproj 2048 subvar angproj 2047
      setvarvar angproj1 angproj
      setvarvar ANGVAR1 ANGVAR
      ifvarvarl ANGVAR angproj 
      subvarvar angproj1 ANGVAR1
